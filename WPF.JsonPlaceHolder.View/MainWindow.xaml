<Window x:Class="WPF.JsonPlaceHolder.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF.JsonPlaceHolder.View"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1000">
    <TabControl Name="TabControlMain" Margin="12" FlowDirection="LeftToRight" TabStripPlacement="Top" Background="LightGray">
        <TabItem Name="Posts" Header="Posts">
            <Frame Name="FramePosts" Source="UserControls/PostsUC.xaml"/>
        </TabItem>
    </TabControl>
</Window>
